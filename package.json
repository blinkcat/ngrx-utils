{
  "name": "@ngrx-utils/platform",
  "version": "0.11.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ibazel run //projects/example/src:devserver",
    "build": "ng build --prod",
    "build:bazel": "bazel build projects/...",
    "test": "ng test",
    "test:bazel": "bazel query --output=label projects/... | xargs bazel test",
    "lint": "ng lint",
    "validate:commit": "commitlint -e $GIT_PARAMS",
    "e2e": "ng e2e",
    "cz": "git-cz",
    "format": "pretty-quick",
    "gulp": "gulp",
    "postinstall": "ngc -p ngc.tsconfig.json",
    "prebuildifier": "bazel build --noshow_progress :buildtools",
    "buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"*/node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/platform-server": "^6.0.7",
    "@angular/router": "^6.0.7",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "core-js": "^2.5.7",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular-devkit/build-ng-packagr": "^0.6.8",
    "@angular/bazel": "^6.0.7",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/language-service": "^6.0.7",
    "@angular/upgrade": "^6.0.7",
    "@bazel/ibazel": "^0.4.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@commitlint/prompt-cli": "^7.0.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.3",
    "@types/glob": "^5.0.35",
    "@types/gulp": "^4.0.5",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.10.20",
    "angular-quokka-plugin": "^1.0.1",
    "chalk": "^2.4.1",
    "codecov": "4.0.0-0",
    "codelyzer": "^4.4.2",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "http-server": "^0.11.1",
    "husky": "0.15.0-rc.13",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-spec-reporter": "^0.0.32",
    "lint-staged": "^7.2.0",
    "minimist": "^1.2.0",
    "ng-packagr": "^3.0.3",
    "prettier": "^1.13.6",
    "pretty-quick": "^1.6.0",
    "protractor": "^5.3.2",
    "puppeteer": "^1.5.0",
    "reflect-metadata": "^0.1.12",
    "resolve-bin": "^0.4.0",
    "ts-node": "^6.2.0",
    "tsconfig-paths": "^3.4.1",
    "tsickle": "^0.29.0",
    "tslib": "^1.9.3",
    "tslint": "^5.10.0",
    "tsutils": "^2.27.1",
    "typescript": "~2.7.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run format -- --staged",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "quokka": {
    "plugins": "angular-quokka-plugin"
  }
}
